{
    "name": "navigator-landing",
    "private": true,
    "scripts": {
      "dev": "npm-run-all --parallel serve watch",
      "serve": "live-server --port=5173 --open=./index.html",
      "watch": "tailwindcss -c tailwind.config.js -i ./css/tailwind.css -o ./css/tailwind-runtime.css --watch",
    "build": "tailwindcss -c tailwind.config.js -i ./css/tailwind.css -o ./css/tailwind-runtime.css --minify"
    },
    "devDependencies": {
      "autoprefixer": "^10.4.20",
      "live-server": "^1.2.2",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.47",
      "tailwindcss": "^3.4.13"
    }
  }
  
